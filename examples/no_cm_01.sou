  const zero = 0
  const one = 1
  mut x = 3
  mut y = 7
  mut z = 1
  mut i = 0
  mut acc = 0
  mut w = 0
  read w
  const limit = 10
loop:
  branch (i == limit) continue loop_body
loop_body:
  branch (w == zero) l1 l2
l1:
  # This statement cannot be moved (but the entire if-branch can)
  z <- (x + y)
l2:
  acc <- (acc + z)
  i <- (i + one)
  goto loop
continue:
print acc
