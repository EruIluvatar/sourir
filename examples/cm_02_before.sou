# Moving a loop invariant
# BEFORE
  var one = 1
  mut x = 3
  mut y = 7
  mut z = 0
  mut i = 0
  mut acc = 0
  mut limit = 10
  read limit
  print z
loop:
  branch (i == limit) continue loop_body
loop_body:
  # This assignment can be moved
  z <- (x + y)
  acc <- (acc + z)
  i <- (i + one)
  goto loop
continue:
print acc
