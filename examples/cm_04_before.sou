  var zero = 0
  var one = 1
  var x = 3
  var y = 7
  var z = 1
  var i = 0
  var acc = 0
  var w = 0
  read w
  var limit = 10
loop:
  branch (i == limit) continue loop_body
loop_body:
  branch (w == zero) l1 l2
l1:
  # This statement cannot be moved (but the entire if-branch can)
  z <- (x + y)
l2:
  acc <- (acc + z)
  i <- (i + one)
  goto loop
continue:
print acc
